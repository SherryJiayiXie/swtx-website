{
    "version": "0.2.0",
    "configurations": [

        {
            "type": "node",
            "request": "launch",
            "name": "node 启动",
            "program": "${workspaceRoot}/bin/www"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome",
            "url": "http://localhost:3000/route-to-gym.html",
            "webRoot": "${workspaceRoot}"
        },
        {
            "name": "Launch index.html",
            "type": "edge",
            "request": "launch",
            "file": "${workspaceRoot}/index.html"
        },
        {
            "name": "Launch localhost with sourcemaps",
            "type": "edge",
            "request": "launch",
            "url": "http://localhost:3000/index.html",
            "sourceMaps": true,
            "webRoot": "wwwroot"
        },
        {
            "name": "Attach",
            "type": "edge",
            "request": "attach",
            "port": 9222
        }
    ]
}